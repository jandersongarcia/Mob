{
    "name": "jandersongarcia/mob",
    "description": "Modular Object Builder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Janerson Garcia",
            "email": "jganjos.info@gmail.com"
        }
    ],
    "homepage": "https://github.com/jandersongarcia/mob",
    "autoload": {
        "classmap": [
            "core/Class"
        ]
    },
    "version": "1.0.0",
    "require": {
        "php": ">=7.0",
        "matthiasmullie/minify": "^1.3",
        "twbs/bootstrap": "5.3.2",
        "twbs/bootstrap-icons": "^1.10",
        "phpmailer/phpmailer": "^6.9"
    },
    "scripts": {
        "mob-create-page": "php core/Php/Composer/create-page.php",
        "mob-delete-page": "php core/Php/Composer/delete-page.php",
        "mob-create-component": "php core/Php/Composer/create-component.php",
        "mob-delete-component": "php core/Php/Composer/delete-component.php",
        "mob-create-module": "php core/Php/Composer/create-module.php",
        "mob-delete-module": "php core/Php/Composer/delete-module.php",
        "mob-list-routes": "php core/Php/Composer/routes-list.php",
        "mob-rename-route": "php core/Php/Composer/routes-rename.php",
        "mob-theme-install": "php core/Php/Composer/create-theme-install.php"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
