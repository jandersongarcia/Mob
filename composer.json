{
    "name": "jandersongarcia/mob",
    "description": "Modular Object Builder",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Janerson Garcia",
            "email": "jganjos.info@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "core/Class/Application.php",
            "core/Class/Mob.php",
            "core/Class/Root.php"
        ]
    },
    "dependencies": {
        "jquery": "^3.6.0",
        "navigo": "^7.1.0"
    },
    "version": "1.2.0",
    "require": {
        "php": ">=7.0",
        "matthiasmullie/minify": "^1.3",
        "twbs/bootstrap": "5.3.2",
        "twbs/bootstrap-icons": "^1.10",
        "phpmailer/phpmailer": "^6.9"
    },
    "scripts": {
        "post-install-cmd": [
            "npm install",
            "App\\ScriptHandler::movePackage"
        ],
        "mob-create-page": "php core/Php/Composer/CreatePage.php",
        "mob-delete-page": "php core/Php/Composer/DeletePage.php",
        "mob-create-component": "php core/Php/Composer/CreateComponent.php",
        "mob-delete-component": "php core/Php/Composer/DeleteComponent.php",
        "mob-create-module": "php core/Php/Composer/CreateModule.php",
        "mob-delete-module": "php core/Php/Composer/DeleteModule.php",
        "mob-theme-install": "php core/Php/Composer/CreateTheme.php",
        "list-routes": "php core/Php/Composer/RouteList.php",
        "rename-route": "php core/Php/Composer/RouteRename.php",
        "maestro": "php core/Php/Composer/MobGenerator.php",
        "component": "php core/Php/Composer/MobComponent.php",
        "module": "php core/Php/Composer/MobModules.php",
        "pages": "php core/Php/Composer/MobPages.php"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
